        function encodeQueryParameters(url) {
            var urlParts = url.split("?");
            if (urlParts.length !== 2) {
                return url;
            }

            var baseUrl = urlParts[0];
            var query = urlParts[1];
            var queryParams = query.split("&");

            var encodedParams = queryParams.map(function (param) {
                var keyValue = param.split("=");
                var key = keyValue[0];
                var value = encodeURIComponent(keyValue.slice(1).join("="));
                return key + "=" + value;
            });

            return baseUrl + "?" + encodedParams.join("&");
        }

        function decodeQueryParameters(url) {
            var urlParts = url.split("?");
            if (urlParts.length !== 2) {
                return url;
            }

            var baseUrl = urlParts[0];
            var query = urlParts[1];
            var queryParams = query.split("&");

            var decodedParams = queryParams.map(function (param) {
                var keyValue = param.split("=");
                var key = decodeURIComponent(keyValue[0]);
                var value = decodeURIComponent(keyValue.slice(1).join("="));
                return key + "=" + value;
            });

            return baseUrl + "?" + decodedParams.join("&");
        }
