                <input type="text" id="CellPhone" name="CellPhone" maxlength="10" style="width: 120px; margin: 0 10px"
                  placeholder="10 chữ số" ng-model="ctrl.model.CreateModel.CellPhone"
                  ng-disabled="ctrl.model.IsSendCaptcha || ctrl.model.IsRunningCustomerServiceState || ctrl.model.IsCellPhoneOverSendTime"
